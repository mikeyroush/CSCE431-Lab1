<%= error_messages_for(f.object) %>

<table summary="Book form fields">
    <tr>
        <th>Title</th>
        <td><%= f.text_field :title %></td>
    </tr>
    <tr>
        <th>Author</th>
        <td><%= f.text_field :author %></td>
    </tr>
    <tr>
        <th>Genre</th>
        <td><%= f.text_field :genre %></td>
    </tr>
    <tr>
        <th>Price</th>
        <td><%= f.text_field :price %></td>
    </tr>
    <tr>
        <th>Published Date</th>
        <td><%= f.text_field :published_date, data: {provide:'datepicker'} %></td>
    </tr>
</table>